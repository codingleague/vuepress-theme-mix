{
  "name": "vuepress-theme-mix",
  "version": "2.0.0-alpha.6",
  "description": "A VuePress theme with a mix of features",
  "keywords": [
    "vuepress-theme",
    "vuepress",
    "theme",
    "mix"
  ],
  "homepage": "https://github.com/gavinliu6/vuepress-theme-mix",
  "bugs": {
    "url": "https://github.com/gavinliu6/vuepress-theme-mix/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gavinliu6/vuepress-theme-mix.git"
  },
  "license": "MIT",
  "author": "Gavin Liu <hello@gavinliu.me> (https://gavinliu.me)",
  "type": "module",
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib",
    "templates"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json",
    "clean": "rimraf lib *.tsbuildinfo",
    "copy": "cpx \"src/**/*.{d.ts,vue}\" lib",
    "style": "tailwindcss -i src/client/styles/index.css -o lib/client/styles/index.css"
  },
  "dependencies": {
    "@vuepress/client": "2.0.0-beta.64",
    "@vuepress/core": "2.0.0-beta.64",
    "@vuepress/plugin-container": "2.0.0-beta.64",
    "@vuepress/plugin-git": "2.0.0-beta.64",
    "@vuepress/plugin-medium-zoom": "2.0.0-beta.64",
    "@vuepress/plugin-nprogress": "2.0.0-beta.64",
    "@vuepress/plugin-shiki": "2.0.0-beta.64",
    "@vuepress/plugin-theme-data": "2.0.0-beta.64",
    "@vuepress/shared": "2.0.0-beta.64",
    "@vuepress/utils": "2.0.0-beta.64",
    "@vueuse/core": "^10.2.1",
    "clsx": "^1.2.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
